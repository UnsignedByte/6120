main {
.unknown:
	in: count=?
	out: count=?, neg_two=-2, one=1, x=1, zero=0
.guard:
	in: cond=gt count zero, count={?, sub count one}, neg_two=-2, one=1, x={1, mul x neg_two}, zero=0
	out: cond=gt count zero, count={?, sub count one}, neg_two=-2, one=1, x={1, mul x neg_two}, zero=0
.body:
	in: cond=gt count zero, count={?, sub count one}, neg_two=-2, one=1, x={1, mul x neg_two}, zero=0
	out: cond=gt count zero, count=sub count one, neg_two=-2, one=1, x=mul x neg_two, zero=0
.end:
	in: cond=gt count zero, count={?, sub count one}, neg_two=-2, one=1, x={1, mul x neg_two}, zero=0
	out: cond=gt count zero, count={?, sub count one}, neg_two=-2, one=1, x={1, mul x neg_two}, zero=0
}


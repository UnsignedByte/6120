@main(iters: int) {
  x: int = undef;
  one.0.0: int = const 1;
  i.0.1: int = const 0;
  x.1.shadow: int = id x;
  i.1.shadow: int = id i.0.1;
.head:
  cond.1.0: bool = lt i.1.shadow iters;
  br cond.1.0 .body .end;
.body:
  print i.1.shadow;
  x.2.1: int = add i.1.shadow one.0.0;
  i.2.2: int = id x.2.1;
  x.1.shadow: int = id x.2.1;
  i.1.shadow: int = id i.2.2;
  jmp .head;
.end:
  print i.1.shadow iters;
}
